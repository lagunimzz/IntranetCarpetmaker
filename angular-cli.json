        "../node_modules/font-awesome/css/font-awesome.css",
        "../node_modules/ionicons/dist/css/ionicons.min.css",

        "../src/assets/css/superfish.css",
        "../src/assets/css/prettyPhoto.css",
        "../src/assets/css/animations.css",
        "../src/assets/css/colors/blue.css",
        "../src/assets/css/theme-responsive.css",
        "../src/assets/css/switcher.css",
        "../src/assets/css/spectrum.css",
        "../src/assets/css/style.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/tether/dist/js/tether.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
